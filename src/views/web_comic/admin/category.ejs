<form method="post">
    <input type="text" name="id" value="<%= category.id %>" hidden>
    <div class="mb-3">
        <label for="name" class="form-label">Name</label>
        <input type="text" ng-model="name" name="name" value="<%= category.name %>" class="form-control" id="name" aria-describedby="emailHelp">
    </div>
    <button type="submit" class="btn btn-primary" formaction="/admin/category/insert">Insert</button> <button type="submit"
        formaction="/admin/category/update" class="btn btn-success">Update</button> <a href="/admin/category">reset</a>
</form>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Created At</th>
            <th>Updated At</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody class="table-hover">
        <% categories.forEach(element => { %>
            <tr>
                <td><%= element.id %></td>
                <td><%= element.name %></td>
                <td><%= element.createdAt %></td>
                <td><%= element.updatedAt %></td>
                <td><a href="/admin/category/edit/<%= element.id %>">Edit</a></td>
                <td><form action="/admin/category/delete/<%= element.id %>" method="post"><button>Delete</button></form></td>
            </tr>
        <% }) %>
        
    </tbody>
</table>